{
  "expo": {
    "name": "weather-app",
    "slug": "react-native-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.ico",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  },
  "ios": {
    "bundleIdentifier": "com.fh.weatherApp"
  },
  "android": {
    "package": "com.fh.weatherApp"
  }
}
